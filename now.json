{
  "version": 2,
  "functions": {
    "api/main.py": {
      "runtime": "now-python",
      "includeFiles": "db.sqlite3"
    }
  },
  "builds": [
    {
      "src": "api/main.py",
      "use": "@now/python"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "api/main.py"
    }
  ],
  "github": {
    "silent": true
  }
}